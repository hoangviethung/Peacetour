block var

doctype html

html(lang="en")
	head
		title #{title}
		meta(charset="UTF-8")
		meta(name="viewport", content="width=device-width, initial-scale=1.0, user-scalable=no")
		meta(http-equiv="X-UA-Compatible", content="ie=edge")
		link(href="https://fonts.googleapis.com/css?family=Roboto+Condensed:300,300i,400,400i,700,700i&display=swap&subset=cyrillic,cyrillic-ext,greek,greek-ext,latin-ext,vietnamese" rel="stylesheet")
		link(rel="stylesheet", href="./css/core.min.css")
		link(rel="stylesheet", href="./css/main.min.css")
	body
		block loading
		// HEADER
		block header
			include _header.pug
		block banner,block-book-tour,_tour-type,breadcrumb
		// MAIN
		main
			block main
			#js-page-verify(class=bodyClass hidden)
		// DANH SÁCH ĐỐI TÁC
		include _brand
		// FOOTER
		block footer
			include _footer.pug
		// HOT LINE POPUP
		include _hot-line-popup
		// LOGIN & REGISTER POPUP
		include _login-register-popup
		// ĐIỆN THOẠI FIXED
		#phone-call
			a(data-fancybox data-src="#hot-line-popup" href="javascript:;")
				.phone-fixed.is-animating
		// BACK TO TOP
		#back-top
			img(src="./assets/icons/back-top.png")
		// OVERLAY
		#overlay
		//- include scripts
		//- script(type="text/javascript", src='https://connect.facebook.net/vi_VN/sdk.js#xfbml=1&version=v3.3')
		script(type="text/javascript", src="https://maps.googleapis.com/maps/api/js?&libraries=places&key=AIzaSyAq6ag0zUMblc-Wk5Z5S8KWkHpT3ak1LzI")
		script(type="text/javascript", src="./js/core.min.js")
		script(type="text/javascript", src="./js/main.min.js")
		block scripts